{
  "name": "cdk-dynamo-table-viewer",
  "version": "0.1.12",
  "description": "A demo AWS CDK construct which exposes a DynamoDB table as an HTTP endpoint",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jsii",
    "watch": "jsii -w",
    "prepublish": "npm run build",
    "package": "jsii-pacmak"
  },
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "software.amazon.awscdk.samples.tableviewer",
        "maven": {
          "groupId": "software.amazon.awscdk.samples",
          "artifactId": "tableviewer"
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eladb/cdk-dynamo-table-viewer.git"
  },
  "keywords": [
    "aws-cdk",
    "cdk",
    "construct",
    "dynamodb"
  ],
  "author": {
    "email": "benisrae@amazon.com",
    "name": "Elad Ben-Israel"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/eladb/cdk-dynamo-table-viewer/issues"
  },
  "homepage": "https://github.com/eladb/cdk-dynamo-table-viewer#readme",
  "devDependencies": {
    "@types/node": "^10.12.18",
    "jsii": "^0.7.14",
    "jsii-pacmak": "^0.7.14"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^0.22.0",
    "@aws-cdk/aws-dynamodb": "^0.22.0",
    "@aws-cdk/aws-lambda": "^0.22.0",
    "@aws-cdk/cdk": "^0.22.0"
  },
  "peerDependencies": {
    "@aws-cdk/aws-apigateway": "^0.22.0",
    "@aws-cdk/aws-dynamodb": "^0.22.0",
    "@aws-cdk/aws-lambda": "^0.22.0",
    "@aws-cdk/cdk": "^0.22.0"
  }
}
